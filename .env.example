# Example environment variables for SecureAuthHub
# Copy to .env and fill in your real secrets for local/dev use

# === Backend (FastAPI) ===
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=secureauthhub
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password

S3_ENDPOINT=https://s3.amazonaws.com
S3_BUCKET=your-bucket-name
S3_ACCESS_KEY=your-access-key
S3_SECRET_KEY=your-secret-key

ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USER=your-es-user
ELASTICSEARCH_PASSWORD=your-es-password

UNSTRUCTURED_API_KEY=your-unstructured-api-key

JWT_SECRET=your-jwt-secret
JWT_ALGORITHM=HS256

# === Frontend (Vite/React) ===
VITE_API_URL=http://localhost:8000

# === Other (Optional) ===
REDIS_URL=redis://localhost:6379


